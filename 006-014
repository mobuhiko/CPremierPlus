#include <stdio.h>
#include <math.h>

int main(void) {
	double a[8], b[8];

	printf("enter 8 elements for the first array.\n");
	int i;
	for (i = 0; i < 8; i++) {
		scanf("%lf", &a[i]);
	}

	double sum = 0;
	for (i = 0; i < 8; i++) {
		sum += a[i];
		b[i] = sum;
	}

	printf("a[8] = ");
	for (i = 0; i < 8; i++) {
		printf("%.2lf ", a[i]);
	}

	printf("b[8] = ");
	for (i = 0; i < 8; i++) {
		printf("%.2lf ", b[i]);
	}

	return 0;
}
