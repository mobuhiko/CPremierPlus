#include <stdio.h>

int main(void) {
	int min, max;
	printf("enter the minimum\n");
	scanf("%d", &min);
	printf("enter the maximum\n");
	scanf("%d", &max);
	if (min > max) {
		printf("please restart the program and enter correctly\n");
		exit();
	}

	for (min; min <= max; min++) {
		printf("%10d %10d %10d\n", min, min * min, min * min * min);
	}

	return 0;
}
